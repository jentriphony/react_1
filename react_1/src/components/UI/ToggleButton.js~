const Button = dataProps {



	let status = dataProps
	let type = dataProps.buttonType
	let classes = dataProps.classes
	let title = dataProps.buttonTitle

	if(type === undefined) {
		type = 'button'
	}
	if(className === undefined) {
		className = ''
	}
	if(title === undefined) {
		title = 'submit'
	}


	return (

<button type={ type } className={ classes }>
 { title }
</button>

	)



}



export default Button
